{
  "compilerOptions": {
    "alwaysStrict": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strict": true,
    "target": "es6"
  },
  "exclude": ["node_modules"]
}
